<p id="notice"><%= notice %></p>

<h1>Enfoques</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enfoques.each do |enfoque| %>
      <tr>
        <td><%= enfoque.nombre %></td>
        <td><%= link_to 'Show', enfoque %></td>
        <td><%= link_to 'Edit', edit_enfoque_path(enfoque) %></td>
        <td><%= link_to 'Destroy', enfoque, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enfoque', new_enfoque_path %>
