<p id="notice"><%= notice %></p>

<h1>Trabajo Grados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trabajo_grados.each do |trabajo_grado| %>
      <tr>
        <td><%= trabajo_grado.nombre %></td>
        <td><%= link_to 'Show', trabajo_grado %></td>
        <td><%= link_to 'Edit', edit_trabajo_grado_path(trabajo_grado) %></td>
        <td><%= link_to 'Destroy', trabajo_grado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trabajo Grado', new_trabajo_grado_path %>
