<p id="notice"><%= notice %></p>

<h1>Grupo Investigacions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grupo_investigacions.each do |grupo_investigacion| %>
      <tr>
        <td><%= grupo_investigacion.nombre %></td>
        <td><%= link_to 'Show', grupo_investigacion %></td>
        <td><%= link_to 'Edit', edit_grupo_investigacion_path(grupo_investigacion) %></td>
        <td><%= link_to 'Destroy', grupo_investigacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grupo Investigacion', new_grupo_investigacion_path %>
